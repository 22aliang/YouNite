<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.basicfunctions01.mapper.ItemMapper">
    <resultMap id="ItemEntityMap" type="com.example.basicfunctions01.entity.Item">
        <id column="id" property="id"></id>
        <result column="item_name" property="itemName"></result>
        <result column="price" property="price"></result>
    </resultMap>


    <select id="selectAll" resultMap="ItemEntityMap">
        SELECT * FROM `item`;
    </select>



</mapper>